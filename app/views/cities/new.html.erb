<div align="center">
    <h1>CREATE A NEW CITY</h1>
    <h1>🌎 🌍 🌏</h1>
    <%= form_with local: true, method: "post", url: "create" do |form| %>
        <p>City:                 <%= form.text_field :name %></p>
        <p>Population:           <%= form.number_field :population %></p>
        <p>Most Famous Landmark: <%= form.text_field :landmark %></p>
    	<%= form.submit %>
    <% end %>
    <br>
    <p>⭑ ⭑ ⭑</p>
    <%= form_with local: true, method: "post", url: "main" do |form| %>
    	<%= form.submit "Return to main page" %>
    <% end %>
</div>
